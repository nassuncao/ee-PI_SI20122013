@model PI_MVC.Models.User

    @if (@ViewData["Role"] != null && @ViewData["Role"].Equals("admin") 
        && @ViewData["user"] != null && !@ViewData["user"].Equals(User.Identity.Name))
    {
        @Html.ActionLink("Togle Role", "ChangeRole", new { userId = @Model.NickName})
    }
    else{
      @Html.ActionLink("Edit Profile", "EditProfile", new { userId = @Model.NickName })
      @Html.Label("", "|")
      @Html.ActionLink("Change Pass","ChangePassword","Account")
     }
    @Html.Label("", "|")
    @Html.ActionLink("Delete User","Delete",new {userId = @Model.NickName})